<thead>
    <tr>
        <th>@AccountNumber</th>
        <th>@AccountHolderName</th>
        <th>@Balance</th>
        <th>@TypeOfAccount</th>
    </tr>
</thead>

@code {
    [Parameter]
    public string AccountNumber { get; set; }
    [Parameter]
    public string AccountHolderName { get; set; }
    [Parameter]
    public string Balance { get; set; }
    [Parameter]
    public string TypeOfAccount { get; set; }
}
